<Text ux:Class="Feature" ux:Name="feature" Clicked="{selectFeature}" Alignment="CenterRight" FontSize="18" Font="Regular" TextColor="#bbb" Padding="20" HitTestMode="LocalBounds">
	<WhileTrue Value="{selected}">
		<Change feature.TextColor="#fff" Duration="0.1"/>
	</WhileTrue>
	<DropShadow Color="#000f" Size="5" Spread="0.2" Angle="90" Distance="1" />
</Text>